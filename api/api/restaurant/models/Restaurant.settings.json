{
  "connection": "default",
  "collectionName": "restaurants",
  "info": {
    "name": "restaurant",
    "description": "Restaurant listings"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "required": true,
      "type": "richtext"
    },
    "categories": {
      "collection": "category",
      "via": "restaurants"
    },
    "restaurant_hours": {
      "group": "hours_of_operation",
      "repeatable": true,
      "required": true,
      "type": "group"
    }
  }
}